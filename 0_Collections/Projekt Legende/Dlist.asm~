* DISPLAYIST FUER PLAYFIELD* (LEGENDE)* BUFFER   EQU $5000  BILDRAM*DBUFLO	EQU 772   BUFFER LOW DBUFHI	EQU 773   BUFFER HIDAUX1	EQU 778   SEKTORNUMMER LOWDAUX2	EQU 779   SEKTORNUMMER HIDCOMND	EQU 770	 KOMMANDODSKINV	EQU 58451 IO ROTINEPLAYF 	EQU $A000 PLAYFIELD.*	ORG $A800	JMP START*DLIST	DFB 112+128		DFB 6+64	DFW NAME	DFB 32+128	DFB 14+64	DFW BUFFER	DFB 14,14,14+128,14,14,14+128,14,14		DFB 14,14,14+128,14,14,14+128,14,14	DFB 14,14,14+128,14,14,14+128,14,14	DFB 14,14,14+128,14,14,14+128,14,14	DFB 14,14,14+128,14,14,14+128,14,14	DFB 14,14,14+128,14,14,14,14,14	DFB 14,14,14,14,14,14,14,14	DFB 14,14,14,14,14+128	DFB 4+64	DFW PLAYF	DFB 4,4,4,4,4,4,4,4,4,4,4,4,4,4	DFB 4+128	DFB 13+128	DFB 2+64	DFW SCORE	DFB 2,2	DFB 65** LOADSCREEN*LOADSCR	DFB 112,112,112,112,112,112	DFB 7+64	DFW LSCREEN	DFB 7	DFB 112	DFB 6+64+32LNAME	DFB 0,0	DFB 65	DFW LOADSCR** NAME DES LEVELS UND SCOREANZEIGE*NAME	ASC %ÓÅÉ vorsichtig !!!!!%SCORE	ASC %PUNKTE: 00000000         LEBEN: 01      %	ASC %                                        %	ASC $BF'89 SOFTWAREPROJEKTE ALLER ART!!!!    $** ADRESSEN DER LEVE NAMES*ADR	DFB L1:L,L2:L,L3:L,L4:L	DFB L5:L,L6:LADRH	DFB L1:H,L2:H,L3:H,L4:H	DFB L5:H,L6:H** NAMEN DER LEVEL 1-6*L1	ASC %GEHT ES LOS!........%L2	ASC %GEHT WEITER IM TAKT.%L3	ASC %GET ES RUND!!!......%L4	ASC %KOMT NOCH EIN LEVEL?%L5	ASC $GET ES RICHTIG LOS! $L6	ASC $DAS FINALE!(ENDLICH)$	*LSCREEN	ASC $DAS SPIEL BEGINNT...$	ASC $UND JETZT:          $** SEKTORTABELLE FUER BACKGROUNG GR.*STAB	DFB 421:L,462:L,503:L,544:L	DFB 585:L,626:LSTABH	DFB 421:H,462:H,503:H,544:H	DFB 585:H,626:H*LAENGE	DFB 0 ANZAHL DER SEKTORENLEVEL	DFB 3ZAELER	DFB 0 FUER VSCROLL** LADEROTINE FUER:* ZEICHENSATZ 1-3* BACKGROUND GRAFIK* UND PLAYFIELD** LADE PLAYFIELD*START	JSR SLSCR	LDX LEVEL  NUMMER DES LEVELS	LDA STAB,X   SEKTORLOW 		STA DAUX1	LDA STABH,X  SEKTORHI	STA DAUX2	LDA #PLAYF:L BUFFERLOW	STA DBUFLO	LDA #PLAYF:H BUFFER HI	STA DBUFHI     	LDA #5	STA LAENGE* LOADLOADBACK	JSR DSKINV	JSR INCBUFF  BUFFER+128	JSR SEKTOR	DEC LAENGE	LDA LAENGE   ENDE?	CMP #0	BNE LOADBACK NOE ->WEITER.** ZEICHENSAETZE LADEN*	LDA #ZSATZ:L	STA DBUFLO	LDA #ZSATZ:H	STA DBUFHI	LDA #18	STA LAENGE*LOADCH	JSR DSKINV	JSR SEKTOR	JSR INCBUFF	DEC LAENGE	LDA LAENGE	CMP #0	BNE LOADCH	JSR UPSCROLL  TEXT WEG	JMP RUN** BUFFER UM 128 BYTES ERHOEHEN.*INCBUFF	CLC	LDA DBUFLO	ADC #128	STA DBUFLO	LDA DBUFHI	ADC #0	STA DBUFHI	RTS** SEKTOR +1*SEKTOR	CLC	LDA DAUX1	ADC #1	STA DAUX1	LDA DAUX2	ADC #0	STA DAUX2	RTS** SUBROTINE -LOADSCREEN ZEIGEN*SLSCR	LDX LEVEL  LEVELNAME	LDA ADR,X  AUSGEBEN.	STA LNAME	LDA ADRH,X	STA LNAME+1*	LDA #LOADSCR DLIST EIN.	STA 560	LDA #LOADSCR/256	STA 561*	LDA #8	STA 54277   VSCROLL	STA ZAELERSCROLL	DEC ZAELER   TEXT RUNTER-	LDA ZAELER   SCROLLEN	STA 54277	JSR WARTEN	LDA ZAELER	CMP #0	BNE SCROLL	RTS*UPSCROLL	LDA #0       TEXT RAUF-	STA 54277    SCROLLEN.	STA ZAELERUPSC	INC ZAELER	LDA ZAELER	STA 54277	JSR WARTEN	LDA ZAELER	CMP #7	BNE UPSC	RTS*WARTEN	LDA #250     KURZ WARTEN.	STA $14WART	LDA $14	BNE WART	RTS** AKTIVIERE DLI UND ZEIGE PLAYFIELD*RUN	LDA #DLIST	STA 560	LDA #DLIST/256	STA 561*	LDA #DLI1   1. DLI ROTINE	STA 512	   AUFRUFEN.	LDA #DLI1/256	STA 513	LDA #192	STA 54286*	JMP $5800*------------------------------------* DISPLAY INTERUPT UNTERPROGRAMME** -DLI1:GRAPHICS 0 ZEILE RAINBOW COLOR* -DLI2:SPIELFELD BLAU (DUNKEL)* -DLI3:SPIELFELD BLAU (HELL  )*-------------------------------------*COLPF0	EQU $D016COLPF1	EQU $D017COLPF2	EQU $D018COLPF4	EQU $D01AVCOUNT	EQU $D40BWSYNC	EQU $D40AZEIGER	EQU $D409DLIVKTL	EQU 512DLIVKTH	EQU 513ZSATZ	EQU 18432*DLI1	PHA        ALLE REG.VOM STACK	STA WSYNC	LDA #3	STA COLPF2	STA COLPF4	LDA #224     BEKOMMT FEST SP.{	STA ZEIGER	LDA #DLI2:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI2:H	STA DLIVKTH	PLA        RETURN F.INTERUPT.	RTI** INTERUPT 2*DLI2	PHA	STA WSYNC	LDA #4	STA COLPF4	LDA #DLI3:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI3:H	STA DLIVKTH	PLA	RTI	** INTERUPT 3*DLI3	PHA	STA WSYNC	LDA #5	STA COLPF4	LDA #DLI4:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI4:H	STA DLIVKTH	PLA	RTI** INTERUPT 4 *DLI4	PHA	STA WSYNC	LDA #6	STA COLPF4	LDA #DLI5:L	STA DLIVKTL	LDA #DLI5:H	STA DLIVKTH	PLA	RTI ** INTERUPT 5*DLI5	PHA	STA WSYNC	LDA #7	STA COLPF4	LDA #DLI6:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI6:H	STA DLIVKTH	PLA	RTI** INTERUPT 6*DLI6	PHA	STA WSYNC	LDA #8	STA COLPF4	LDA #DLI7:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI7:H	STA DLIVKTH	PLA	RTI** INTERUPT 7*DLI7	PHA	STA WSYNC	LDA #9	STA COLPF4	LDA #50	STA COLPF2	LDA #DLI8:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI8:H	STA DLIVKTH	PLA	RTI	** INTERUPT 8*DLI8	PHA	STA WSYNC	LDA #10	STA COLPF4	LDA #DLI9:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI9:H	STA DLIVKTH	PLA	RTI** INTERUPT 9 *DLI9	PHA	STA WSYNC	LDA #11	STA COLPF4	LDA #DLI10:L	STA DLIVKTL	LDA #DLI10:H	STA DLIVKTH	PLA	RTI ** INTERUPT 10*DLI10	PHA	STA WSYNC	LDA #12	STA COLPF4	LDA #DLI11:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI11:H	STA DLIVKTH	PLA	RTI** INTERUPT 11*DLI11	PHA        ALLE REG.VOM STACK	STA WSYNC	LDA #13	STA COLPF4	LDA #DLI12:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI12:H	STA DLIVKTH	PLA        RETURN F.INTERUPT.	RTI** INTERUPT 12*DLI12	PHA	STA WSYNC	LDA #14	STA COLPF4	LDA #DLI13:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI13:H	STA DLIVKTH	PLA	RTI	** INTERUPT 13*DLI13	PHA	STA WSYNC	LDA #50	STA COLPF4	LDA #DLI14:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI14:H	STA DLIVKTH	PLA	RTI** INTERUPT 14 *DLI14	PHA	STA WSYNC	LDA #5	STA COLPF4	LDA #ZSATZ:H	STA ZEIGER	LDA #DLI15:L	STA DLIVKTL	LDA #DLI15:H	STA DLIVKTH	PLA	RTI ** INTERUPT 15*DLI15	PHA	STA WSYNC	LDA #1	STA COLPF4	LDA #50	STA COLPF2	LDA #DLI16:L	STA DLIVKTL	LDA #DLI16:H	STA DLIVKTH	PLA	RTI** INTERUPT 16*DLI16	PHA	STA WSYNC	LDA #25	STA COLPF2	STA COLPF4	LDA #224	STA ZEIGER	LDA #DLI1:L  ZEIGER AUF NEUHE	STA DLIVKTL  ROTINE.	LDA #DLI1:H	STA DLIVKTH	PLA	RTIÿ